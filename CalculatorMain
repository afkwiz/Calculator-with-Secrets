    private void ClearButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                   
    TextDisplay.setText("");
    firstnum = 0;
    secondnum = 0;
    result = 0;
    operations = ".";
    banDecimals = 0;
    }                                                  

    private void PlusButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                  
    banDecimals = 0;
    firstnum = Double.parseDouble(TextDisplay.getText());
    TextDisplay.setText("");
    operations = "+";
    }                                                 

    private void MinusButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                   
    banDecimals = 0;
    firstnum = Double.parseDouble(TextDisplay.getText());
    TextDisplay.setText("");
    operations = "-";
    }                                                  

    private void MultiplyButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                      
    banDecimals = 0;
    firstnum = Double.parseDouble(TextDisplay.getText());
    TextDisplay.setText("");
    operations = "*";
    }                                                     

    private void DivideButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                    
    banDecimals = 0;
    firstnum = Double.parseDouble(TextDisplay.getText());
    TextDisplay.setText("");
    operations = "/";
    }                                                   

    private void DecimalButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                     
    if (TextDisplay.getText().equals("")){
        banDecimals = 0;
    } else {
    if (banDecimals == 0){
    String entnum = TextDisplay.getText() + DecimalButtonPressed.getText();
    TextDisplay.setText(entnum);  
    }
    banDecimals += 1;
    }
    }                                                    

    private void EqualsButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                    
    String answer;
//    banDecimals += 1;
    secondnum = Double.parseDouble(TextDisplay.getText());
    switch (operations){
        case "+": result = firstnum + secondnum;
                  answer = String.format("%.2f", result);
                  TextDisplay.setText(answer);
                  break;
        case "-": result = firstnum - secondnum;
                  answer = String.format("%.2f", result);
                  TextDisplay.setText(answer);
                  break;
        case "*": result = firstnum * secondnum;
                  answer = String.format("%.2f", result);
                  TextDisplay.setText(answer);
                  break;
        case "/": result = firstnum / secondnum;
                  answer = String.format("%.7f", result);
                  TextDisplay.setText(answer);
                  break;
        case ".": TextDisplay.setText(TextDisplay.getText());   
                  break;
        default: TextDisplay.setText(TextDisplay.getText());     
    }
    }                                                   

    private void EqualsButtonPressed1ActionPerformed(java.awt.event.ActionEvent evt) {                                                     
    System.exit(0);
    }                                                    

    private void ClearButtonPressed1ActionPerformed(java.awt.event.ActionEvent evt) {                                                    
    new MoreFunctions().setVisible(true);
    this.dispose();
    }                                                   

    private void SquareButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                    
    firstnum = Double.parseDouble(TextDisplay.getText());
    result = Math.pow(firstnum, 2);
    TextDisplay.setText(String.valueOf(result));
    }                                                   

    private void SqrtButtonPressedActionPerformed(java.awt.event.ActionEvent evt) {                                                  
    firstnum = Double.parseDouble(TextDisplay.getText());
    result = Math.sqrt(firstnum);
    TextDisplay.setText(String.valueOf(result));
    }                                                 

